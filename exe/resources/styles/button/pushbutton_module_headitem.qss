QPushButton#moduleHeadItem
{
	border-top:1px solid #CCCCCC;
	background-color: #FFFFFF;
	/*font-family: "Arial";*/
	color: #666666;
	font-size: 12px;
	text-align: left; 
	padding-left: 20px;
}
QPushButton#moduleHeadItem:hover
{
	background-color: #FFFFFF;
}
QPushButton#moduleHeadItem:pressed
{
	background-color: #FFFFFF;
}
QPushButton#moduleHeadItem:checked
{
	color: #398dee;
}